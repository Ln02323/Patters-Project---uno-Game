package uno;

import java.util.LinkedList;

/**
 *
 * @author WinDows
 */
public class BasePlayer implements Player {
    private LinkedList<Card> cards = new LinkedList<>();

    public LinkedList<Card> getCards() {
        return cards;
    }
}

